{
  "name": "@35up/http-client",
  "version": "1.1.3",
  "main": "dist/http-client.node.cjs.js",
  "module": "dist/http-client.esm.js",
  "types": "dist/types",
  "exports": {
    "import": {
      "node": "./dist/http-client.node.esm.js",
      "default": "./dist/http-client.esm.js"
    },
    "require": {
      "node": "./dist/http-client.node.cjs.js",
      "default": "./dist/http-client.cjs.js"
    }
  },
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint --ext .ts,.js .",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "test:ci": "npm test -- --ci --forceExit --runInBand",
    "test:coverage": "npm test -- --coverage",
    "prepublishOnly": "npm i && npm run build && npm run lint && npm run test",
    "ci": "npm i && npm run build && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/35up/http-client.git"
  },
  "keywords": [
    "http",
    "client"
  ],
  "author": "35up GmbH <the.engineers@35up.com>",
  "bugs": {
    "url": "https://github.com/35up/http-client/issues"
  },
  "homepage": "https://github.com/35up/http-client#readme",
  "devDependencies": {
    "@35up/eslint-config-35up-base": "^4.0.0",
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/chai": "^4.2.0",
    "@types/jest": "^26.0.15",
    "@types/sinon-chai": "^3.2.5",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "chai": "^4.2.0",
    "core-js": "^3.7.0",
    "eslint": "^8.22.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "ts-mock-imports": "^1.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "node-fetch": "^3.2.10"
  }
}
